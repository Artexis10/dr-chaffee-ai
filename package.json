{
  "name": "ask-dr-chaffee",
  "version": "1.0.0",
  "description": "Dr. Chaffee AI Assistant - Single Instance Deployment",
  "scripts": {
    "setup": "node scripts/setup.js",
    "setup:backend": "node scripts/setup-backend.js",
    "setup:frontend": "cd frontend && npm install",
    "docker:setup": "docker-compose -f docker-compose.dev.yml up -d --build",
    "docker:start": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:stop": "docker-compose -f docker-compose.dev.yml down",
    "docker:restart": "docker-compose -f docker-compose.dev.yml restart",
    "docker:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "docker:reset": "docker-compose -f docker-compose.dev.yml down -v && docker-compose -f docker-compose.dev.yml up -d",
    "docker:ingest": "docker-compose -f docker-compose.dev.yml exec backend python scripts/ingest_youtube.py --limit 10",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "cd frontend && npm run dev",
    "start:backend": "cd backend && python -m uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "start:docker": "docker-compose up -d",
    "stop": "docker-compose down",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && python -m uvicorn main:app --reload",
    "build": "cd frontend && npm run build",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd frontend && npm test",
    "test:backend": "pytest tests/unit/",
    "ingest": "cd backend && python scripts/ingest_youtube.py",
    "ingest:test": "cd backend && python scripts/ingest_youtube.py --limit 10",
    "db:reset": "docker-compose down -v && docker-compose up -d",
    "db:status": "docker-compose ps"
  },
  "dependencies": {
    "concurrently": "^8.2.2"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
